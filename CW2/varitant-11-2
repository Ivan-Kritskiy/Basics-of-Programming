#include <iostream>
using namespace std;
void fill(int** matrix, int n, int n2) {
    for (int i = 0; i < n; i++) {
        matrix[i] = new int[n2];
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n2; j++) {
            cin >> matrix[i][j];
        }
    }
}
int main()
{
    int n, n2;
    int line_min;
    int* arr = new int[n];
    int** matrix = new int* [n];
    fill(matrix, n, n2);
    for (int i = 0; i < n; i++) {
        line_min = matrix[0][0];
        for (int j = 0; j < n2; j++) {
            if (line_min > matrix[i][j]) {
                line_min = matrix[i][j];
            }
        }
        int vert_max = matrix[0][j];
        for (int k = 0; k < n; k++) {
            if (vert_max < matrix[k][j]) {
                break;
            }
            else {
                arr[i] = vert_max;
            }
            
        }
    }
      for (int i = 0; i < n, i++) {
        cout << arr[i] << " ";
    }
}
}
